<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OwnerHomePage">

    <ListView
        android:id="@+id/ListView"
        android:layout_width="313dp"
        android:layout_height="0dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="41dp"
        android:layout_marginBottom="49dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/displayownerage" />

    <TextView
        android:id="@+id/displayowneremail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="146dp"
        app:layout_constraintEnd_toStartOf="@+id/displayownerage"
        app:layout_constraintStart_toStartOf="@+id/displayownerage"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayownerage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="@+id/ListView"
        app:layout_constraintTop_toBottomOf="@+id/displayowneremail" />

    <TextView
        android:id="@+id/displayownername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/displayowneremail"
        app:layout_constraintEnd_toStartOf="@+id/displayowneremail"
        app:layout_constraintStart_toStartOf="@+id/displayowneremail" />

    <Button
        android:id="@+id/NewBusiness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="25dp"
        android:text="New"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:text="Your Businesses"
        app:layout_constraintStart_toStartOf="@+id/ListView"
        app:layout_constraintTop_toTopOf="@+id/ListView" />

    <Button
        android:id="@+id/editownerprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="21dp"
        android:text="Edit Profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>